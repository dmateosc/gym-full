{
  "version": 2,
  "name": "gym-exercise-backend",
  "builds": [
    {
      "src": "apps/backend/src/main.ts",
      "use": "@vercel/node",
      "config": {
        "includeFiles": [
          "apps/backend/dist/**"
        ]
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "apps/backend/src/main.ts"
    },
    {
      "src": "/(.*)",
      "dest": "apps/backend/src/main.ts"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  },
  "build": {
    "env": {
      "NODE_ENV": "production"
    }
  }
}
