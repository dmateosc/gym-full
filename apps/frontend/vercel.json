{
  "version": 2,
  "name": "gym-exercise-frontend",
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "framework": "vite",
  "env": {
    "VITE_API_BASE_URL": "@vite_api_base_url"
  },
  "build": {
    "env": {
      "VITE_API_BASE_URL": "@vite_api_base_url"
    }
  },
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
