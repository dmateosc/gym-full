{
  "version": 2,
  "builds": [
    {
      "src": "dist/main.vercel.js",
      "use": "@vercel/node",
      "config": {
        "includeFiles": ["dist/**"]
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "dist/main.vercel.js"
    },
    {
      "src": "/(.*)",
      "dest": "dist/main.vercel.js"
    }
  ],
  "installCommand": "npm install",
  "buildCommand": "npm run build"
}
