{
  "version": 2,
  "builds": [
    {
      "src": "src/main.vercel.ts",
      "use": "@vercel/node",
      "config": {
        "includeFiles": ["dist/**"]
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "src/main.vercel.ts"
    },
    {
      "src": "/(.*)",
      "dest": "src/main.vercel.ts"
    }
  ],
  "installCommand": "npm install",
  "buildCommand": "npm run build",
  "functions": {
    "src/main.vercel.ts": {
      "runtime": "nodejs20.x"
    }
  }
}
