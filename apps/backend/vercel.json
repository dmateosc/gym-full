{
  "version": 2,
  "builds": [
    {
      "src": "api/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "api/index.js"
    },
    {
      "src": "/(.*)",
      "dest": "api/index.js"
    }
  ],
  "installCommand": "npm install --include=dev",
  "buildCommand": "npm run build && mkdir -p api && cp dist/main.vercel.js api/index.js"
}
